[req]
default_bits       = 2048
prompt             = no
default_md         = sha256
distinguished_name = dn
req_extensions     = req_ext

[dn]
C  = US
ST = FL
L  = Orlando
O  = ShortURL
CN = shorturl.local

[req_ext]
subjectAltName = @alt_names

[alt_names]
DNS.1 = localhost
DNS.2 = shorturl.local
IP.1  = 127.0.0.1
IP.2  = 192.168.1.1